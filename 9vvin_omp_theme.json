{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "newline": true,
      "alignment": "left",
      "leading_diamond": "",
      "trailing_diamond": "",
      "segments": [
        {
          
          "background": "#5A86AD",
          "background_templates": [
            "{{ if gt .Code 0 }}#BE2D26{{ end }}"
          ],
          "foreground": "#FFF9D5",
          "style": "diamond",
          "leading_diamond": "",
          "template": " {{ .UserName }} ",
          "type": "text"
        },
        {
          "background": "#E0DBB7",
          "foreground": "#573D26",
          "properties": {
            "mapped_locations_enabled": true,
            "home_icon": "🛖",
            "style": "full",
            "mapped_locations": {
              "C:\\Users\\march\\workbench": "🛠️",
              "C:\\Users\\march\\workbench\\living": "💸",
              "C:\\Users\\march\\workbench\\me": "🐇"
            }
          },
          "style": "diamond",
          "leading_diamond": "",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#6BA18A",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#9B6C4A{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#BE2D26{{ end }}",
            "{{ if gt .Ahead 0 }}#93CFD7{{ end }}",
            "{{ if gt .Behind 0 }}#9B6C4A{{ end }}"
          ],
          "foreground": "#2A1F1D",
          "properties": {
            "branch_icon": " ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "mapped_branches": {
              "main": "👑",
              "master": "👑",
              "prd": "👑",
              "product": "👑",
              "production": "👑",
              "dev": "🛠️",
              "development": "🛠️",
              "staging": "💃",
              "stage": "💃",
              "stg": "💃",
              "*-main": "👑",
              "*-master": "👑",
              "*-prd": "👑",
              "*-product": "👑",
              "*-production": "👑",
              "*-dev": "🛠️",
              "*-development": "🛠️",
              "*-staging": "💃",
              "*-stage": "💃",
              "*-stg": "💃"
            }
          },
          "style": "diamond",
          "leading_diamond": "",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#E99D2A",
          "foreground": "#FFF9D5",
          "leading_diamond": "",
          "style": "diamond",
          "template": "<i> 👀 {{ .FormattedMs }} </i>",
          "alias": "exetime",
          "type": "executiontime",
          "properties": {
            "always_enabled": false
          }
        },
        {
          "background": "#D0D150",
          "foreground": "#2A1F1D",
          "style": "diamond",
          "leading_diamond": "",
          "template": "{{ if .Segments.exetime }} ⌚ {{ .CurrentDate | date .Format }} {{ else }}{{ end }}",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#5A86AD",
          "background_templates": [
            "{{ if gt .Code 0 }}#BE2D26{{ end }}"
          ],
          "foreground": "#FFF9D5",
          "style": "diamond",
          "leading_diamond": " ",
          "trailing_diamond": "",
          "template": "{{ if .Root }}🛡️{{ else }}💲{{ end }}",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 3,
  "terminal_background": "#2A1F1D",
  "accent_color": "#C07020"
}